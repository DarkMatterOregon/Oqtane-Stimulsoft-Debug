@using DarkMatter.Stimtest.Services
@using DarkMatter.Stimtest.Models

@using Stimulsoft.Base
@using Stimulsoft.Report
@using Stimulsoft.Report.Blazor

@namespace DarkMatter.StimtestDashboard
@inherits ModuleBase
@inject IStimtestService StimtestService
@inject NavigationManager NavigationManager
@inject IStringLocalizer<Index> Localizer

<h3>Sample Dashboard</h3>
<StiBlazorViewer Report="@dashboard" />

@code {
    private StiReport dashboard;


    protected override void  OnInitialized()
    {
        Stimulsoft.Base.StiLicense.Key = "";

        //Create empty dashboard object
        dashboard = StiReport.CreateNewDashboard();

        //Load dashboard template
        dashboard.Load("Reports/DashboardChristmas.mrt");

    }

}